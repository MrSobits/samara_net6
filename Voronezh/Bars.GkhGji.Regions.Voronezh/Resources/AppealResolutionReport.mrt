        <Parameters isList="true" count="0" />
        <SqlCommand>select gas.revenue_date,
	gas.revenue_source_number,
	gac.date_from,
	gac.gji_number,
	gdss.name,
	gac.correspondent,
	gac.correspondent_address
       
	from gji_appeal_citizens gac
