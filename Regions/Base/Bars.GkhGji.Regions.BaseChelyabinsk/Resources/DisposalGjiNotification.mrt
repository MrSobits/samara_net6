      <УправляющаяОрганизация Ref="5" type="Stimulsoft.Report.Dictionary.StiPostgreSQLSource" isKey="true">
        <Alias>УправляющаяОрганизация</Alias>
        <Columns isList="true" count="5">
          <value>Наименование,System.String</value>
          <value>ИНН,System.String</value>
          <value>ЮридическийАдрес,System.String</value>
          <value>ФИОРуководителяДП,System.String</value>
          <value>ДолжностьРуководителяДП,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>УправляющаяОрганизация</Name>
        <NameInSource>Connection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>select 
	distinct on (1,2,3,4)
c.name "Наименование",
c.inn "ИНН",
c.juridical_address "ЮридическийАдрес",
case when nullif(trim(cc.surname_dative),'') is null
	then cc.full_name
	else array_to_string(array[cc.surname_dative, cc.name_dative, cc.patronymic_dative], ' ')
	end "ФИОРуководителяДП",
upper(substring(coalesce(nullif(trim(p.name_dative),''), p.name)  from '^.')) || substring(coalesce(nullif(trim(p.name_dative),''), p.name)  from '^.(.*)$') "ДолжностьРуководителяДП"
from gji_document d
left join gji_inspection i on i.id=d.inspection_id
left join gkh_contragent c on c.id=i.contragent_id
left join gkh_contragent_contact cc on cc.contragent_id=c.id
left join gkh_dict_position p on p.id=cc.position_id
where d.id={ИдентификаторДокументаГЖИ}
order by 1,2,3,4,p.code asc</SqlCommand>
      </УправляющаяОрганизация>
